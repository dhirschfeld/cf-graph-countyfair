    "fortran_compiler_stub",
    "make",
    "pkg-config",
    "cmake >=3.12"
   ],
   "host":[
    "nompi",
    "mpi4py",
    "python",
    "numpy >=1.15.0",
    "libffi",
    "hdf5",
    "hdf5  >=1.8.13 = nompi_*",
