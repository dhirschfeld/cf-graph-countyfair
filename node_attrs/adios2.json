 "linux_64_meta_yaml":{
  "about":{
   "description":"ADIOS2 is the latest implementation of the ADaptable Input Output System,\nADIOS. This brand new architecture was designed to continue supporting the\nperformance legacy of ADIOS, and extend its current capabilities to address\ncurrent and future input/output (IO) challenges in the scientific data\nlifecycle through effective research and development (R&D) activities.\n",
   "dev_url":"https://github.com/ornladios/ADIOS2",
   "doc_url":"https://adios2.readthedocs.io",
   "home":"https://www.olcf.ornl.gov/center-projects/adios/",
   "license":"Apache-2.0",
   "license_file":"LICENSE",
   "summary":"Next generation of ADIOS developed in the Exascale Computing Program"
  },
  "build":{
   "number":"101",
   "run_exports":[
    "adios2 * nompi_*",
    "adios2"
   ],
   "string":"nompi_pyh_101"
  },
  "extra":{
   "recipe-maintainers":[
    "ax3l",
    "chuckatkins",
    "williamfgc"
   ]
  },
  "package":{
   "name":"adios2",
   "version":"2.6.0"
  },
  "requirements":{
   "build":[
    "c_compiler_stub",
