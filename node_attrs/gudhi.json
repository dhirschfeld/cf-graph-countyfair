  "about":{
   "description":"The GUDHI library is a generic open source C++ library, with a Python\ninterface, for Topological Data Analysis (TDA) and Higher Dimensional\nGeometry Understanding. The library offers state-of-the-art data structures\nand algorithms to construct simplicial complexes and compute persistent\nhomology.\n",
   "doc_url":"http://gudhi.gforge.inria.fr/python/3.2.0/",
   "home":"http://gudhi.gforge.inria.fr/",
   "license":"MIT AND BSD-3-Clause AND MPL-2.0 AND LGPL-3.0-or-later AND GPL-3.0-or-later",
   "license_file":"LICENSE",
   "summary":"Geometry Understanding in Higher Dimensions"
  },
  "build":{
   "number":"0"
  },
  "extra":{
   "recipe-maintainers":[
    "VincentRouvreau"
   ]
  },
  "package":{
   "name":"gudhi",
   "version":"3.2.0"
  },
  "requirements":{
   "build":[
    "cmake",
    "c_compiler_stub",
    "cxx_compiler_stub"
   ],
   "host":[
    "git",
    "python",
    "numpy",
    "setuptools",
    "cython",
    "pybind11",
    "boost-cpp",
    "eigen",
    "cgal-cpp 5.0.1"
   ],
   "run":[
    "python",
    "numpy",
    "setuptools",
    "boost-cpp",
    "cgal-cpp 5.0.1"
   ]
  },
  "source":{
   "git_depth":"1",
   "git_rev":"tags/gudhi-release-3.2.0",
   "git_url":"https://github.com/GUDHI/gudhi-devel.git"
  },
  "test":{
   "commands":[
    "test -f ${PREFIX}/include/gudhi/Simplex_tree.h",
    "conda inspect linkages $PKG_NAME",
    "pytest src/python/test/test_alpha_complex.py",
    "pytest src/python/test/test_bottleneck_distance.py",
    "pytest src/python/test/test_cubical_complex.py",
    "pytest src/python/test/test_euclidean_witness_complex.py",
    "pytest src/python/test/test_reader_utils.py",
    "pytest src/python/test/test_rips_complex.py",
    "pytest src/python/test/test_simplex_generators.py",
    "pytest src/python/test/test_simplex_tree.py",
    "pytest src/python/test/test_subsampling.py",
    "pytest src/python/test/test_tangential_complex.py",
    "pytest src/python/test/test_time_delay.py",
